{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-config-prettier"
	],
	"rulesDirectory": [
		"tslint-plugin-prettier"
	],
	"rules": {
		"prettier": true,
		"interface-name": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"member-access": false,
		"jsx-no-lambda": false,
		"import-blacklist": [true, "lodash"]
	}
}
